{
  "name": "discord-user-installable",
  "version": "0.1.0",
  "description": "Represents a Discord User-Installable Command",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "prettier": "prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loldonut/discord-user-installable.git"
  },
  "author": "John Heinrich",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/loldonut/discord-user-installable/issues"
  },
  "homepage": "https://github.com/loldonut/discord-user-installable#readme",
  "dependencies": {
    "@discordjs/builders": "^1.7.0",
    "@sapphire/shapeshift": "^3.9.7"
  },
  "devDependencies": {
    "@types/node": "^20.12.8",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "all"
  }
}
